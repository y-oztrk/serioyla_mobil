<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.creative.cosmonaut">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CreativeCosmonaut">
        <activity
            android:name=".Note_List"
            android:label="@string/title_activity_note__list"
            android:theme="@style/Theme.CreativeCosmonaut.NoActionBar"></activity>
        <activity
            android:name=".NotPadactvy"
            android:label="@string/title_activity_not_padactvy"
            android:theme="@style/Theme.CreativeCosmonaut.NoActionBar" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>